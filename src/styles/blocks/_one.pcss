.link-class {
  width: 100px;
  height: 100px;
  fill: yellow;
}

.icon-bg {
  width: 100px;
  height: 100px;
  cursor: pointer;
  background: svg-load('user.svg', fill=$text-color, width=100%, height=100%);
  &:hover {
    background: svg-load('user.svg', fill=#55ddcc, width=100%, height=100%);
  }
}

.socials__icon {
  width: 30px;
  height: 30px;
  cursor: pointer;
  
}

